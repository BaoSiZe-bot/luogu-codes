#include <stdio.h>

int h[1000005];
char buf[1 << 23], *p1 = buf, *p2 = buf, obuf[1 << 23], *O = obuf;
inline char getc()
{
    return p1 == p2 && (p2 = (p1 = buf) + fread(buf, 1, 1 << 21, stdin), p1 == p2) ? EOF : *p1++;
}
inline void uread(register int &x)
{
    x = 0;
    register char ch = getc();
    for (; ch < 48 || ch > 57; ch = getc())
        ;
    for (; ch > 47 && ch < 58; ch = getc())
        x = (x << 1) + (x << 3) + (ch ^ 48);
}
int main()
{
    int n, m, x;
    scanf("%d%d%d", &n, &m, &x);
    for (int i = 1; i <= n; ++i) 
        scanf("%d", h + i);

    return 0;
}
